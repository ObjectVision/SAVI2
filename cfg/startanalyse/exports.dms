	container top2
	{
		unit<uint32> kosten := SourceData/buurt2023
		{
			parameter<float32> percentage := 0.15f;
			attribute<units/Euro_per_ton_CO2_per_yr> S1H17 := Exports/strats/export_strat1/Buurtindicatoren/H17_Nationale_meerkosten_CO2;
			attribute<units/Euro_per_ton_CO2_per_yr> S2H17 := Exports/strats/export_strat2/Buurtindicatoren/H17_Nationale_meerkosten_CO2;
			attribute<units/Euro_per_ton_CO2_per_yr> S3H17 := Exports/strats/export_strat3/Buurtindicatoren/H17_Nationale_meerkosten_CO2;
			attribute<units/Euro_per_ton_CO2_per_yr> S4H17 := Exports/strats/export_strat8/Buurtindicatoren/H20_toekenning_klimaatneutraal_gas?Exports/strats/export_strat4/Buurtindicatoren/H17_Nationale_meerkosten_CO2:(0/0)[units/Euro_per_ton_CO2_per_yr];
			//attribute<units/Euro_per_ton_CO2_per_yr> S4H17 := Exports/strats/export_strat4/Buurtindicatoren/H17_Nationale_meerkosten_CO2;
			
			attribute<uint32> min_index := argmin(S1H17, S2H17, S3H17, S4H17);
			attribute<float32> best_value := min_elem(S1H17, S2H17, S3H17, S4H17);
			
			attribute<units/Euro_per_ton_CO2_per_yr> S1H17_or_not := min_index==0?(0/0)[units/Euro_per_ton_CO2_per_yr]:S1H17;
			attribute<units/Euro_per_ton_CO2_per_yr> S2H17_or_not := min_index==1?(0/0)[units/Euro_per_ton_CO2_per_yr]:S2H17;
			attribute<units/Euro_per_ton_CO2_per_yr> S3H17_or_not := min_index==2?(0/0)[units/Euro_per_ton_CO2_per_yr]:S3H17;
			attribute<units/Euro_per_ton_CO2_per_yr> S4H17_or_not := min_index==3?(0/0)[units/Euro_per_ton_CO2_per_yr]:S4H17;
			
			attribute<float32> second_best_value := min_elem(S1H17_or_not, S2H17_or_not, S3H17_or_not, S4H17_or_not);
			
			attribute<float32> difference := second_best_value - best_value;
			attribute<bool> is_percentueel_afwijkend := difference / second_best_value < percentage;
		}
	}
	
